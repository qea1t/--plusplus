#include <iostream>

using namespace std;

int main()
{
    float x, y;
r:cout << "enter x and y : " << endl;
    cout << "enter x =";
    cin >> x;
    cout << "enter y =";
    cin >> y;
    if ((x <= 1 and x >= 0 and y <= 0 and y >= (-1)) or (x >= (-1) and x <= 0 and y >= 0 and y <= 1)) cout << "yes";
    else cout << "no";
    cout << endl;
    goto r;
    return 0;
}
